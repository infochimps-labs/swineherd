digraph crust {
  rankdir=BT
  ordering=out
  compound=true 

  node [shape=circle]  bowl
  node [shape=rectangle]
  subgraph clusterCombine {
    label=combine
    "bowl.add(egg)"   -> bowl
    "bowl.add(flour)" -> bowl
    "bowl.add(sugar)" -> bowl
  }
  "mixer.mix(bowl)" -> "bowl.add(sugar)" [ lhead=clusterCombine ]
  "bowl.transfer_contents(pie_tin)" -> "mixer.mix(bowl)"
}
